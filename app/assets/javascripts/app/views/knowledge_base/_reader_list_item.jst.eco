<a href="<%- @item.url %>">
  <span class="section-inner">
    <%- @iconWithModifier(@item, className: 'section-inner-icon-holder', iconset: @iconset, addStateClass: true) %>
    <span><%= @item.title %></span>
    <% if @item.count: %>
      <span>Answers: <%= @item.count %></span>
    <% end %>
  </span>
</a>
